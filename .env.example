# Base URL for the exchange API
BASE_URL=http://localhost:8000

# Rate limit for API requests (requests per second)
RATE_LIMIT=10

# Flag to enable dry-run mode (True or False)
DRY_RUN=False

# Factor for exponential backoff in retry logic
BACKOFF_FACTOR=2

# Number of retry attempts for failed orders
RETRY_ATTEMPTS=3

# Data file paths
ORDERS_CSV_PATH=data/Orders.csv
PRECISION_CSV_PATH=data/Precision.csv

# Exchange Credentials 1
API_KEY_1=DEMO_API_KEY_1
API_SECRET_1=API_SECRET_1
API_ACCOUNT_1=account1@example.com

# Exchange Credentials 2
API_KEY_2=DEMO_API_KEY_2
API_SECRET_2=API_SECRET_2
API_ACCOUNT_2=account2@example.com

# Exchange Credentials 3
API_KEY_3=DEMO_API_KEY_3
